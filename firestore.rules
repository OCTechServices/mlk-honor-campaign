rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    match /payments/{docId} {
      allow read: if true;
      allow write: if false;
    }

    match /messages/{docId} {
      allow read: if true;
      allow write: if false;
    }

    match /{document=**} {
      allow read, write: if false;
    }
  }
}
